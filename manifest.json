{
    "manifest_version": 3,
    "name": "Test Extension",
    "version": "1.0",
    "action": {
        "default_popup": "popup/popup.html",
        "default_icon": "icon.png"
    },
    "background": {
        "service_worker": "background/dist/background.js"
    },
    "permissions": [
        "alarms",
        "storage",
        "tabs"
    ],
    "host_permissions": [
        "<all_urls>"
    ]
}
